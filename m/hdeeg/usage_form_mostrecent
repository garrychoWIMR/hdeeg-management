    <script>		

		function textAreaAdjust__ID(o) {
			//console.log(o)
			o.style.height = "1px";
			o.style.height = (25+o.scrollHeight)+"px";
		}
			//-------------------------------------
    	function F__ID(){
    		//-------------------------------------
            VmInclude:__COMPONENT__/f/form.01.js
			VmInclude:__COMPONENT__/f/form-item-a.01.js
			VmInclude:__CURRENT_PATH__/purchase-form.html.item_a_list.js

			//-------------------------------------
			m.before_submit = function (data, index) {
				data.staff = 'Researcher: '+ data.staff_r + '\n' + 'Sleep Technician: '+data.staff_st;
				research_study = ($('#research_study__ID').val());
				study_id = ($('#study_id__ID').val());
				medicared = ($('#medicared__ID').val())
				additional_studies = ($('#more_studies__ID').val());
				type_of_study=($('#study_type__ID').val());
				alert("Research study is " + research_study + "\n"
				+ "Study id  id " + study_id + "\n" +
				"Is this Study medicared? " + medicared + "\n"+
				"Type of Study is : "+ type_of_study


				)

			
			
			}
		//-------------------------------------
		$('#D__ID').on('load',function(){
			var today = new Date();
			var dd = String(today.getDate()).padStart(2, '0');
			var mm = String(today.getMonth() + 1).padStart(2, '0'); //January is 0!
			var yyyy = today.getFullYear();

			today = dd + '/' + mm + '/' + yyyy;
			$('#date__ID').val(today);
		//-------------------------------------
			
		//-------------------------------------
		// Prefill studies via URL
			var url=window.location.href
			var url_1=url.split('&').pop()
			var url_1= url.split('=').pop();
			
			if (url_1==undefined){
			document.getElementById('research_study__ID').readOnly = false;
			// console.log("hello")
			}
			else{
			// var url_2= url_1.split('=')[1]
			var research_study=url_1.replace("%20"," ");
			document.getElementById('research_study__ID').readOnly = true;
			console.log('research study is ' + research_study)
			$('#research_study__ID').val(research_study);
			}
			
			//-------------------------------------
			$("#scoredd__ID").hide();
			$('input[name=scoring_needed]').on('change', function () {
				var scoring_needed = ($('#nd_scoring__ID'))
				// var scoring_needed_n = ($('#nd_scoring__ID'))
				// $('#needs_score__ID')
				//alert(scoring_needed.val())
				if (scoring_needed.is(':checked')){
					$("#scoredd__ID").show()
					
				
				}
				else {
					$("#scoredd__ID").hide()
					}

				})
			}) 
	
			//******************************************************//
			// To find the values and then add them to the list for studies to be scored
			// Will potentially need to take a step further and add the functionality of adding several studies?
			//  Logic: 
			// 1. Are there any additional studies (?)
			// 2. type of studies (mwt, kdt, etc. )
			// 3. Number of studies 
			// 4. Add it in as a new entry?


			// Add a mini table which they can designate the number of rows for scoring
			//
			//
			//******************************************************

			
			// for (var i_k=0;i_k<11;i_k++){
			i_k=0
			$("#item_a_add__ID").on('click',function(){
			i_k++

			var study_id = "<input type='text' class=form-control name=study_id id=study_id_"+i_k+"__ID></input>"
			var checkbox="<center><input type='checkbox' id=medicared_"+i_k+"__ID></input></center></td><td><center>"
			var study_type= "<center><select id=study_type_"+i_k+"__ID class=form-control><option>PSG</option><option>KDT</option><option>MWT</option></select></center>"

			var table = document.getElementById("grid_item_a__ID")
			var row = table.insertRow()
			var cell1 = row.insertCell(0);
			var cell2 = row.insertCell(1);
			var cell3 = row.insertCell(2);
			var cell4 = row.insertCell(3);
			var cell5 = row.insertCell(4);
			var cell6 = row.insertCell(5);
			cell1.innerHTML = i_k;		
			cell2.innerHTML = $('#research_study__ID').val()
			cell3.innerHTML = study_id;	
			cell4.innerHTML = checkbox ;
			cell5.innerHTML = study_type ;
			cell6.innerHTML = "<center><i class='fa fa-trash' aria-hidden='true' onclick=alert('hello')></i></center>"
 			});
			// function Delete() {
			// document.getElementById("grid_item_a__ID").deleteRow();
			// // }
			// }

			
		

	$("#checker__ID").on('click', function () {
		if (i_k != 0) {
			no_of_studies = i_k
			alert('there are ' + no_of_studies + ' studies')
			// alert(no_of_studies)	
			for (ii=1;ii<no_of_studies+1;ii++){
				// ale.log()
				var si = $("#study_id_"+[ii]+"__ID").val()
				var med= $("#medicared_"+ [ii] +"__ID").val()
				var st= $("#study_type_"+[ii]+"__ID").val()
				var ar=[si.toString(),med.toString(),st.toString()]
				// alert(ar)

			}

			}
		});
	//--------------------------------------
		}
